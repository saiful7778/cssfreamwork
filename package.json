{
  "name": "customecssfreamwork",
  "version": "1.0.0",
  "description": "This is my own custom css freamwork like bootstrap",
  "main": "index.js",
  "scripts": {
    "compile:css": "sass --no-source-map style.scss:css/style.css --style expanded",
    "compress:css": "sass --no-source-map css/style.css:css/style.min.css --style compressed",
    "build": "npm-run-all compile:css compress:css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "sass": "^1.58.3"
  }
}
